/* Do certain publishers stay within a certain genere */

SELECT
	v.name,
	i.publisher,
	i.genre
FROM 
	videogame AS v
INNER JOIN
	info AS i
ON
	v.id = i.videogame_id;
ORDER BY
   	publisher;
